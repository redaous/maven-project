pipeline {
    agent any
    stages{
        stage('Build'){
            steps {
                sh '/opt/apache-maven-3.6.2/bin/mvn clean package'
            }
            post {
                success {
                    echo 'Now Archiving...'
                    archiveArtifacts artifacts: '**/*.war'
                }
            }
        }
        stage('Deploying man'){
           steps {
              build job: 'deploy-to-stage'
           }
        }
    }
}
